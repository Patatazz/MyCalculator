		button[2].setText("3");
		button[2].setFocusable(false);
		

		button[3].setSize(new Dimension(50,50));
		button[3].setText("4");
		button[3].setFocusable(false);
		

		button[4].setSize(new Dimension(50,50));
		button[4].setText("5");
		button[4].setFocusable(false);
		

		button[5].setSize(new Dimension(50,50));
		button[5].setText("6");
		button[5].setFocusable(false);
		
		button[6].setSize(new Dimension(50,50));
		button[6].setText("7");
		button[6].setFocusable(false);
		
		button[7].setSize(new Dimension(50,50));
		button[7].setText("8");
		button[7].setFocusable(false);
		
		button[8].setSize(new Dimension(50,50));
		button[8].setText("9");
		button[8].setFocusable(false);
		
		button[9].setSize(new Dimension(50,50));
		button[9].setText("0");
		button[9].setFocusable(false);
	}

	@Override
	public void actionPerformed(ActionEvent e) {
		
		switch (e.getSource()==button[]) {
		
		
		}
		
		
		
		
	}

}
